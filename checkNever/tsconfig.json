{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es2021", "dom"
    ],
    "jsx": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "baseUrl": ".",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "incremental": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}
